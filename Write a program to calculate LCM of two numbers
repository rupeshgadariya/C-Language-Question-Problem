// Write a program to calculate LCM of two numbers

#include<stdio.h>
#include<conio.h>

    void main()
{
        int x,y,count;
        printf("Enter the number ");
        scanf("%d%d",&x,&y);
    count = x>y?x:y;
    
    for (int i = count; 1 ; i++)
      if (i%x==0 && i%y==0)
      {
        printf("LCM of %d and %d = %d ",x,y,i);
        break;
      }
    
    getch();
}
