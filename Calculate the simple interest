#include<stdio.h>
#include<conio.h>

    void main()
{
    float P,R,T;
                        //Calculate the simple interest
   
   printf("Enter the principal value P:\n");
   scanf("%f",&P);
   printf("Enter the principal value R:\n");
  scanf("%f",&R);
    printf("Enter the principal value T:\n");
  scanf("%f",&T);
//S=P*R*T/100;
   printf("Calculate the simple interest = %f",(P*R*T)/100);

getch();
}
