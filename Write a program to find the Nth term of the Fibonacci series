// Write a program to find the Nth term of the Fibonacci series. 

#include<stdio.h>
#include<conio.h>

    void main()
{    
        int x,a=-1,b=1,sum=0;
        printf("Enter Nth term of the fibonacci number ");
        scanf("%d",&x);
       
        for (int i = 1; i <= x ; i++)
        {   sum = a+b;
           a=b,b=sum;
        }
             printf("%d ",sum);
     
    getch();
}
