//  C program to display armstrong number between two intervals 
#include<stdio.h>

    int main()
{
     int a, b, num, lar;
     printf("Enter two number : ");
     scanf("%d%d",&a,&b);
    a>b? (num = b+1) : (num = a+1);
    a>b? (lar = a) : (lar = b);
for (; num<lar; num++ )
{        
    int temp, pro, count=0, sum=0, digit;
    temp = num;

    while (temp)
    {
        count++;
        temp = temp/10;
    }
       temp = num;

    while (temp)
    {       digit = temp%10;
            pro = 1;
        for (int i =0; i<count; i++)
         {
            pro = pro * digit;
         }

        sum = sum + pro;
        temp = temp/10;
    }

    if (sum == num)
     printf("%d   ",num);
}       
    return 0;
}
