    //  C language to multiple two Floating point numbers

#include<stdio.h>
#include<conio.h>
     void main()
{
     
        
         float x,y;
    
    printf("Enter the value in x = "); 
    scanf("%f",&x);
    printf("\rEnter the value in y = ");
    scanf("%f",&y);

printf("Multiple %.3f and %.3f is %.2f",x,y,x*y);    

    getch();
}
