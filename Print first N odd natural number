#include<stdio.h>
#include<conio.h>

  // Print first N odd natural number 

  void main()
{
    int number;
    printf("Enter number ");
    scanf("%d",&number);
    printf("\nFirst %d odd natural number",number);
    for (int i = 0; i <= 2*number; i++)
    {
      if (i%2!=0)
      {
        printf("\n%d",i);
      }
      
    }
  getch();
}
