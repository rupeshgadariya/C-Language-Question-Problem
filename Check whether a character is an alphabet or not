    //  C program to check whether a character is an alphabet or not 


#include<stdio.h>
#include<conio.h>
 
    void main()
{
    char c;
   printf("Enter any character ");
   scanf("%c",&c);
   
   if (c>='a' && c<='z' || c>='A' && c<='Z' )
   {
    printf("This is Alphabet");
   }
   else
   printf("This is not Alphabet");
   
    getch();
}
