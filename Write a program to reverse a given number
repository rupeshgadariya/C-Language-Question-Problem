// Write a program to reverse a given number

#include<stdio.h>
#include<conio.h>

    void main()
{
        int x,y=0;
        printf("Enter the number ");
        scanf("%d",&x);
       while (x)
       {
        y = y*10 + x%10;
        x=x/10;
       }
    printf("Reverse a given number = %d ",y);
        

    getch();
}
