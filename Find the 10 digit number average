#include<stdio.h>
#include<conio.h>
//Que:- Find the 10 digit average in c language

    void main()
{
    float avg;
    int a[10],i,sum=0;                       // aaray is a group of variable

    printf("print 10 number : ");
    for ( i = 0; i<=9; i++)
    {
       scanf("%d",&a[i]);
    }
    for ( i = 0; i<=9; i++)
    {
       sum=sum+a[i];
    }

    avg=sum/10.0;


   printf("The avarage of 10 number = %.2f",avg);
        
        
    getch();
}
