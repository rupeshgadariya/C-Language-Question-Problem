// Write a program to calculate HCF of two numbers

#include<stdio.h>
#include<conio.h>

    void main()
{
        int a,b,c;
        printf("Enter two number ");
        scanf("%d%d",&a,&b);
         if (a<b)
                    a += b - (b=a);
         
       while (1)
       {
            c = a%b;
        if (c==0)
            {
                printf("HCF of given number = %d ",b);
                break;
            }
        else 
            a=b,b=c;
           
       }
       
    getch();
}
