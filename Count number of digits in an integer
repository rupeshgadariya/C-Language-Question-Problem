    //  C program to count number of digits in an integer 

#include<stdio.h>
#include<conio.h>

    void main()
{

        int i=1,x;
        printf("Count number 1 to ");
        scanf("%d",&x);
        while (i<=x)
        {
           printf("\n%d",i);
           i++;
        }
        
    getch();
}
