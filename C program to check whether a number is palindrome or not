//  C program to check whether a number is palindrome or not 

#include<stdio.h>

    int main()
{
         int num, rev=0, temp;
    printf("Enter a number ");
    scanf("%d",&num);
    temp = num;

    while (temp)
    {
        rev =  rev*10 + temp%10;
        temp = temp/10;
    }

    if (rev == num)
     printf("%d is palindrome number",num);
    else 
     printf("%d is not palindrome number",num);

        
    return 0;
}
