// Write a program to print first N terms of Fibonacci series 

#include<stdio.h>
#include<conio.h>

    void main()
{    
        int x,a=-1,b=1,sum=0;
        printf("Enter N term of the fibonacci series ");
        scanf("%d",&x);
   
        for (int i = 1; i <= x ; i++)
        {   sum = a+b;
            printf("%d ",sum);
           a=b,b=sum;
        }
     
    getch();
}
